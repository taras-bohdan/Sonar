{
  "name": "sonar-server",
  "version": "1.0.0",
  "main": "./app/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": "https://github.com/taras-bohdan/Sonar",
  "scripts": {
    "build": "babel app --out-dir ./dist --source-maps",
    "serve": "node ./dist/index.js",
    "client": "cd ../client && npm start",
    "server": "npm start",
    "start": "nodemon --exec babel-node ./app/index.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "test": "jest app",
    "coverage": "jest --collectCoverageFrom=app/**.js --coverage app",
    "lint": "eslint --ext=js ./app"
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "apollo-server-koa": "^2.1.0",
    "bcrypt": "^3.0.2",
    "cap": "^0.2.0",
    "dotenv": "^6.1.0",
    "graphql": "^0.13.0",
    "graphql-tools": "^4.0.2",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-passport": "^4.1.1",
    "koa-redis": "^3.1.3",
    "koa-router": "^7.4.0",
    "koa-session": "^5.10.0",
    "mongoose": "^5.3.4",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pcap-parser": "^0.2.1",
    "random-token": "0.0.8",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "concurrently": "^4.0.1",
    "eslint": "^5.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.13.1"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
